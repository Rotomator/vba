Call DoCmd.RunCommand(acCmdWindowHide)

'hide all menu
'DoCmd.ShowToolbar "Ribbon", acToolbarNo

    

End Sub

Private Sub Command1_Click()

'MsgBox "show"
Call DoCmd.SelectObject(acTable, , True)

'DoCmd.ShowToolbar "Ribbon", acToolbarYes





End Sub
